    "unconfig": "^0.3.11"
  },
  "devDependencies": {
    "@murongg/eslint-config": "^0.2.1",
    "@ora-io/cle-lib": "^0.1.1",
    "@ora-io/release-scripts": "^0.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.6",
    "@types/prompts": "^2.4.6",
    "@types/semver": "^7.5.3",
    "assemblyscript": "^0.27.24",
    "consola": "^3.2.3",
    "eslint": "^8.57.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.3.1",
    "zkwasm-toolchain": "^0.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
